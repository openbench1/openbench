# AI Engine API Keys
ANTHROPIC_API_KEY=sk-ant-...
OPENAI_API_KEY=sk-...

# Default AI engine: "claude" or "openai"
AI_ENGINE=claude

# EVMBench API (cloud server deployment)
EVMBENCH_API_URL=https://evmbench.yourdomain.com

# Etherscan/BSCScan API Keys (for fetching verified contract source code)
ETHERSCAN_API_KEY=
BSCSCAN_API_KEY=

# Vercel Postgres
DATABASE_URL=postgresql://...
DATABASE_URL_UNPOOLED=postgresql://...

# Auth.js (generate secret: npx auth secret)
AUTH_SECRET=
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
AUTH_TRUST_HOST=true

# Upstash Redis (for rate limiting)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000
